;; This is such a hack! XD
(__defmacro #__declare (identifier type &rest script-body) ())

__declare print (I)
__declare __quote (L)
__declare __+ (I &rest 1 I)
__declare __if (I I I)
__declare __apply (I &rest 1 I)

(__defun + (a &rest b)
         __if b
         __+ a __apply self b
         a)
__declare + (I &rest 1 I)

(__defun println (arg)
         (print arg)
         (print "\n")
         arg)
__declare println (I)

println "Hello!"
println __quote print
println + + 1 2
          + 3 4
println (+ 1 2 3 4)
